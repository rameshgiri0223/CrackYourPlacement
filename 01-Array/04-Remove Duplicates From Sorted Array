class Solution {
    public int removeDuplicates(int[] nums) {
        int count = 0;
        for(int i = 0; i < nums.length; i++){
            //after last element there will be no next element so i <nums.length-1
            if(i<nums.length-1 && nums[i] == nums[i+1]){
                continue;
            }

            else {
                nums[count] = nums[i];
                count++;
            }
        }
        return count;
        
    }
}
